  "scripts": {
    "build-sw": "node ./src/serviceWorker-build.js",
    "build:css": "tailwindcss -i src/index.css -o src/asset/main.css",
    "build:pwa": "npm run build && npm run build-sw && npm run clean-cra-sw",
    "build": "cross-env NODE_ENV=production npm run build:css && tsc && vite build",
    "check-types": "tsc",
    "clean-cra-sw": "rimraf -f build/precache-manifest.*.js && rimraf -f build/service-worker.js",
    "clean:init": "rimraf -rf package-lock.json && rimraf -rf build && npm run clean && npm run init",
    "clean": "rimraf -rf node_modules",
    "eject": "react-scripts eject",
    "init": "npm install && npm run patch",
    "lint:fix": "eslint src --ext .js,.ts,.tsx --fix",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "patch": "rimraf node_modules/.cache && npx @vm-lib/patch-node-modules -s node_modules_patches -o node_modules",
    "prebuild": "npm run patch",
    "prepare": "husky install",
    "reinstall": "npm run clean && npm install",
    "serve": "vite preview",
    "sonar": "node sonar-scanner.js",
    "start:prod": "pm2 start ecosystem.config.js",
    "start": "npm run patch && vite",
    "stop:prod": "pm2 stop ecosystem.config.js",
    "test": "react-scripts test",
    "validate-typescript": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
